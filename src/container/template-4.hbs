        <header class="header">
            <div class="">
                {{> logo}}
            </div>


            <nav class="header_nav ">
                <div class=" header-nav-right">
                  
                  {{> nav list=header }}
                
                </div>
            </nav>
        </header>

      
    {{#with main}}
        <main class="main"> 
         <img src="{{image}}" alt="Main image" class="main_image">
              <div class="main_content">
                 <h1 class="main_heading">{{heading}}</h1>
                 <p class="main_descriotion">{{descriotion}}</p>
               <a class=" button main_button" href="{{url}}">{{button}}</a>

              </div>
        </main>
    {{/with}}


    <div class="featured">
        {{#each featured}}
              {{> cardFeatured data=this}}
        {{/each}}
     </div>

   <div class="actions">
        {{#each actions}}
          <div class="actions_content">
               <h3 class="actions_heading">{{heading}}</h3>
               <p class="actions_text">{{text}}</p>
         
                <div class="actions_action">
                   <a class=" button actions_button" href="{{url}}">{{button}}</a>
                </div>
           </div>
       {{/each}}
    </div>
    
      


       <footer class=" footer">
           <div class="">
                {{> logo}}
                <span class="footer_copyright">Copyright 2023-2024</span>
           </div>

           {{#each footer}}
               <div class="">
                <span class="footer_title">{{title}}</span>

                {{> nav list=links isVertical=true}}
                
               </div>
           {{/each}}
       </footer>
